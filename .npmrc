# Configurações para Cloudflare Pages
registry=https://registry.npmjs.org/
prefer-offline=false
prefer-online=true

# Configurações para dependências opcionais (necessárias para esbuild)
optional=true
install-links=false
save-exact=false

# Configurações de compatibilidade
legacy-peer-deps=false
fund=false
audit=false
package-lock=true 